<Window x:Class="FF.Vegas.Deploy.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FF.Vegas.Deploy.UI"
        xmlns:localp="clr-namespace:FF.Vegas.Deploy.UI.Pages"
        mc:Ignorable="d"
        Title="Vegas Deploy" Height="270" Width="400" MinHeight="270" MinWidth="300"  Background="#f3f3f3" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Grid Grid.Column="0" >
            <Frame x:Name="frameMain"   NavigationUIVisibility="Hidden" d:DataContext="{d:DesignInstance IsDesignTimeCreatable=True, Type={x:Type localp:MainPage}}" Content="{Binding}"  />
        </Grid>
    </Grid>
</Window>
